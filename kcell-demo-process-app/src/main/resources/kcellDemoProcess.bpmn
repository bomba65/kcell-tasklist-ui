    <bpmn:sequenceFlow id="SequenceFlow_16zx9lt" name="Temperature above 18 C" sourceRef="ExclusiveGateway_1ynm60v" targetRef="PackWarmTask">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression"><![CDATA[ ${temperature >= 18} ]]></bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SequenceFlow_09xirpp" name="Temperature below 18 C" sourceRef="ExclusiveGateway_1ynm60v" targetRef="PackColdTask">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression"><![CDATA[ ${temperature < 18} ]]></bpmn:conditionExpression>
    </bpmn:sequenceFlow>
