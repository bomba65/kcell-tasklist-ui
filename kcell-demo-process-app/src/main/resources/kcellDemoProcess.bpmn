    <bpmn:userTask id="PackWarmTask" name="Pack for warm weather" camunda:formKey="embedded:app:forms/warm.html" camunda:assignee="demo">
      <bpmn:extensionElements>
        <camunda:taskListener class="kz.kcell.bpm.TaskAssignmentListener" event="assignment" />
      </bpmn:extensionElements>
